<h3>Last extraction: <%= extraction.created_at.strftime(t('time.format_extended')) %></h3>
<%= extends :basic_panel_body do %>
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th> <%= t('extraction_datum.columns.field') %> </th>
        <th> <%= t('extraction_datum.columns.value') %> </th>
      </tr>
      </thead>
      <tbody>
      <% extraction.instances[-1].extraction_data.each do |d| %>
          <tr>
            <td> <%= d.field_name %> </td>
            <td> <%= d.value %> </td>
          </tr>
      <% end%>
      </tbody>
    </table>
<% end %>
