<%= extends :basic_form do %>
  <%= form_for([@project, @script_new], :url => project_scripts_path(@project)) do |f|  %>
    <h1><%= t('scripts.new') %></h1>
    <%= f.label(:name) %>
    <%= f.text_field(:name, class: 'form-control') %>
    <div class="login-group">
      <%= f.submit(t('buttons.create'), class: 'btn btn-primary') %>
    </div>
  <% end %>
<% end%>

