<%= extends :basic_panel do %>
    <%= extends :basic_panel_heading do %>
        <h1><%=t('scripts.index')%></h1>
        <%= link_to(t('scripts.new_button'), new_project_script_frequency_path, class: "btn btn-primary" )%>
    <% end %>
    <%= extends :basic_panel_body do %>
        <table class="table table-striped table-hover">
          <thead>
          <tr>
            <th>
              Interval
            </th>
            <th>
              Period
            </th>
            <th>
              First execution
          </th>
          </tr>
          </thead>
          <tbody>
          <% @frequency.each do |f| %>
              <tr>
                <td>
                  <%= f.interval %>
                  <!--<=link_to(s.name, edit_project_script_path(@project.id, s.id) )%>-->
                </td>
                <td>
                  <%= f.period %>
                </td>
                <td>
                  <%= f.first_exec %>
                </td>
              </tr>
          <% end %>
          </tbody>
        </table>
    <% end %>
<% end %>