

<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th>
      <%= t('frequencies.columns.interval') %>
    </th>
    <th>
      <%= t('frequencies.columns.period') %>
    </th>
    <th>
      <%= t('frequencies.columns.first_exec') %>
  </th>
  </tr>
  </thead>
  <tbody>
  <% frequencies.each do |f| %>
      <tr>
        <td>
          <%= f.interval %>
        </td>
        <td>
          <%= f.period %>
        </td>
        <td>
          <%= f.first_exec %>
        </td>
        <td>
          <%=link_to(edit_project_script_frequency_path(project.id, script.id, f.id)) do %>
              <span class="glyphicon glyphicon-pencil"></span>
          <% end %>
        </td>
        <td>
            <%= link_to project_script_frequency_path(project.id, script.id, f.id), method: :delete do %>
              <span class="glyphicon glyphicon-erase"></span>
            <% end %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>
