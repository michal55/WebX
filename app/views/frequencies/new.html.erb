<%= extends :basic_form do %>
    <%= form_for([@frequency_new], url: project_script_frequencies_path(@project, @script)) do |f|  %>
        <h1><%= t('frequencies.new') %></h1>
        <%= f.label(:interval) %>
        <%= f.text_field(:interval, class: 'form-control') %>
        <%= f.label(:period) %>
        <%= f.text_field(:period, class: 'form-control') %>
        <%= f.label(:first_exec) %>
        <%= f.text_field(:first_exec, class: 'form-control') %>
        <div class="login-group">
          <%= f.submit(t('buttons.create'), class: 'btn btn-primary') %>
        </div>
    <% end %>
<% end%>
