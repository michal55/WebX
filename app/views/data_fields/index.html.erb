<%= extends :basic_panel do %>
  <%= extends :basic_panel_heading do %>
    <h1><%=@project.name%></h1>
  <% end %>
  <%= extends :basic_panel_body do %>
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th> <%=t('data_schemas.columns.name')%> </th>
        <th> <%=t('data_schemas.columns.type')%> </th>
      </tr>
      </thead>
      <tbody>
      <%= render 'data_fields/new'%>
      <% @data_field.each do |d| %>
        <tr>
          <td> <%=link_to(d.name, edit_project_data_field_path(@project.id, d.id) )%> </td>
          <td style="width:200px" > <%= d.data_type%> </td>
        </tr>
      <% end %>
      </tbody>
    </table>
  <% end %>
<% end %>
