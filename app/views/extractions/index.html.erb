<%= extends :basic_panel do %>
  <%= extends :basic_panel_heading do %>
    <h1> <%= @script.name %> Extractions </h1>
  <% end %>
  <%= extends :basic_panel_body do %>
    <table class="table table-striped table-hover">
      <thead>
      <tr>
        <th> Created </th>
        <th> Updated </th>
      </tr>
      </thead>
      <tbody>
        <% @extraction.each do |e| %>
          <tr>
            <td>
              <%=link_to(e.created_at,  project_script_extraction_extraction_datum_index_path(@project.id, @script.id, e.id) )%>
            </td>
            <td>
              <%=link_to(e.updated_at,  project_script_extraction_extraction_datum_index_path(@project.id, @script.id, e.id) )%>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  <% end %>
<% end %>
